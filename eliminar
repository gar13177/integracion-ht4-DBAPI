
        if 'token' in params:
            data = get_object_or_404(Usuario, id_usuario=params['token'])
            if not (isinstance(data, Usuario)):
                return Response("No se encontró", status=status.HTTP_404_NOT_FOUND)
            return [data]

        if ('contrasena' in params) and ('nombre' in params):
            data = get_object_or_404(Usuario, nombre=params['nombre'], contrasena=params['contrasena'])
            if not (isinstance(data, Usuario)):
                return Response("No se encontró", status=status.HTTP_404_NOT_FOUND)
            return [data]
        
        
        print (dict(params))
        print (len(dict(params)))
        if len(dict(params)) != 0:
            
            return Response("prueba", status=status.HTTP_404_NOT_FOUND)

        data = Usuario.objects.all()
        return data